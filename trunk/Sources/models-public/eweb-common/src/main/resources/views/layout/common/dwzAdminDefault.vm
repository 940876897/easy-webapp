<!doctype html>
<html>
<!-- 本站全部采用HTML5的规范,如果显示有问题,这个是正常的,请升级浏览器  -->
    <head>
        <meta charset="utf-8"/>
        <meta http-equiv="X-UA-Compatible" content="IE=7" />
		<title>#if($title) ${title} -#end   后台系统平台 </title>
        $iDemand.addJs('html5')
        $iDemand.addJs('jquery')
        $iDemand.addJs('letskillie6')
		$iDemand.addCss('dwz')
		$iDemand.addCss('uploadify')
		$iDemand.addCss('adminCss')
		$iDemand.addJs('dwz')
		$iDemand.addJs('xheditor')
		$iDemand.addJs('uploadify')
		$iDemand.addJs('baseJs')
		$iDemand.addJs('validator')
		$iDemand.addJs('adminJs')
		
		#parse('/views/screen/contain/iDemand.vm')
        ##    $contain.get('/common/contain/iDemand.xhtml')
        
        <style type="text/css">
            ## 需要导航条时打开以下样式
		 #*   #header{height:85px}
		    #leftside, #container, #splitBar, #splitBarProxy{top:90px}
        *#
		</style>
		
	</head>
	
	<body scroll="no">
	    <div id="layout">
	        <!-- 头部 -->
            $contain.get('/admin/header.xhtml')
	        <!-- 左侧 -->
	        <div id="leftside">
                <div id="sidebar_s">
                    <div class="collapse">
                        <div class="toggleCollapse"><div></div></div>
                    </div>
                </div>
                <div id="sidebar">
                    <div class="toggleCollapse"><h2>主菜单</h2><div>收缩</div></div>
            $contain.get('/admin/leftSidebar.xhtml')
                </div>
            </div>
			
                
	        <!-- 中间区 -->
            <div id="container">
                <div id="navTab" class="tabsPage">
                    <div class="tabsPageHeader">
                        <div class="tabsPageHeaderContent"><!-- 显示左右控制时添加 class="tabsPageHeaderMargin" -->
                            <ul class="navTab-tab">
                                <li tabid="main" class="main"><a href="javascript:;"><span><span class="home_icon">我的主页</span></span></a></li>
                            </ul>
                        </div>
                        <div class="tabsLeft">left</div><!-- 禁用只需要添加一个样式 class="tabsLeft tabsLeftDisabled" -->
                        <div class="tabsRight">right</div><!-- 禁用只需要添加一个样式 class="tabsRight tabsRightDisabled" -->
                        <div class="tabsMore">more</div>
                    </div>
                    <ul class="tabsMoreList">
                        <li><a href="javascript:;">我的主页</a></li>
                    </ul>
                    <div class="navTab-panel tabsPageContent layoutBox">
                        <div class="page unitBox">
	        $screen_content
                        </div>
                    </div>
                </div>
            </div>
            
	        <!-- layout区结束-->
	    </div>
	    
	    <!-- 底部区 -->
        $contain.get('/admin/footer.xhtml')
	
	</body>

</html>